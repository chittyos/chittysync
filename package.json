{
  "name": "chittysync",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "packages/*"
  ],
  "repository": "git+https://github.com/chittyos/chittysync.git",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @chittyos/engine dev",
    "start": "pnpm --filter @chittyos/engine start",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "fmt": "pnpm -r fmt"
  }
}
